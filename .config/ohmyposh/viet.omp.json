{
    "Created": "Phunt_Vieg_",
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "white",
                    "style": "plain",
                    "template": "\u256d\u2500",
                    "type": "text"
                },
                {
                    "background": "#58aac4",
                    "foreground": "white",
                    "leading_diamond": "\ue0b6",
                    "style": "diamond",
                    "template": " \uefa7  ",
                    "trailing_diamond": "\ue0b0",
                    "type": "os"
                },
                {
                    "background": "darkGray",
                    "foreground": "white",
                    "style": "diamond",
                    "template": " {{ .UserName }}@{{ .HostName }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "session"
                },
                {
                    "background": "blue",
                    "foreground": "white",
                    "properties": {
                        "style": "full"
                    },
                    "style": "diamond",
                    "template": " {{ .Path }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "path"
                },
                {
                    "background": "#ffb86c",
                    "foreground": "white",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "branch_icon": "",
                        "fetch_stash_count": true,
                        "fetch_status": false,
                        "fetch_upstream_icon": true
                    },
                    "style": "diamond",
                    "template": " \ue725 ({{ .UpstreamIcon }} {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
                    "trailing_diamond": "\ue0b0",
                    "type": "git"
                },
                {
                    "background": "magenta",
                    "foreground": "white",
                    "properties": {
                        "time_format": "15:04"
                    },
                    "style": "diamond",
                    "template": " \u2665  {{ .CurrentDate | date .Format }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "time"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "transparent",
                    "foreground": "white",
                    "style": "plain",
                    "template": "\u2570\u2500<green><b>\u26a1</b>",
                    "type": "text"
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "white",
        "template": "<blue><b>\uf489<b> <cyan><b>\uf0da<b><lightMagenta><b>\uf105<b> "
    },
    "final_space": true,
    "version": 4
}
